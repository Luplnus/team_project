<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/schema/mybatis-3-mapper.dtd">


<mapper namespace="com.bitstudy.app.dao.Venue_movieMapper">

    <!-- 영화관 등록 - vm_b_seqno, vm_name 받아서 저장  -->
    <insert id="insert" parameterType="map">
        INSERT into venue_movie_tbl ( vm_b_seqno, vm_name )
        values ( #{vm_b_seqno}, #{vm_name})
    </insert>

</mapper>